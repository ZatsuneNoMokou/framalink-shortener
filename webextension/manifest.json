{
	"applications": {
		"gecko": {
			"id": "framalinkshortener_dev@zatsunenomokou.eu",
			"update_url": "https://gitlab.com/ZatsuneNoMokou/framalink-shortener/raw/master/dist/framalink_shortener_dev.update.json",
			"strict_min_version": "50.0"
		}
	},
	"manifest_version": 2,
	"name": "Framalink shortener (Dev)",
	"short_name": "FramalinkShortenerDev",
	"description": "__MSG_extShortDesc__",
	"author": "ZatsuneNoMokou",
	"version": "2.0.0",
	"homepage_url": "https://gitlab.com/ZatsuneNoMokou/framalink-shortener",
	"icons": {
		"96": "/icon_96.png",
		"128": "/icon_128.png"
	},
	"background": {
		"scripts": [ "data/options-data.js", "data/options-api.js", "index.js" ]
	},
	"browser_action": {
		"default_icon": {
			"16": "/icon_16.png",
			"48": "/icon_48.png",
			"96": "/icon_96.png",
			"128": "/icon_128.png"
		}
	},
	"content_scripts": [
		{
			"js": [ "data/js/page_contentScript.js" ],
			"matches": [ "http://*/*", "https://*/*" ],
			"run_at": "document_end"
		}
	],
	"default_locale": "en",
	"offline_enabled": false,
	"permissions": [
		"tabs",
		"activeTab",
		"contextMenus",
		"clipboardWrite",
		"notifications",
		"storage",
		"unlimitedStorage",
		"http://*/*",
		"https://*/*"
	],
	"options_ui": {
		"page": "data/options.html",
		"chrome_style": true
	},
	"minimum_chrome_version": "42.0",
	"web_accessible_resources": [ "/icon_128.png" ]
}
